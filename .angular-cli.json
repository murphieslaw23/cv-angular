{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "CV SPA"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/bxslider/dist/jquery.bxslider.css",
        "../node_modules/owlcarousel/owl-carousel/owl.carousel.css",
        "../node_modules/owlcarousel/owl-carousel/owl.theme.css",
        "../node_modules/fancybox/dist/css/jquery.fancybox.css",
        "../node_modules/mediaelement/build/mediaelementplayer.css",
        "assets/fonts/map-icons/css/map-icons.min.css",
        "styles.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/bxslider/dist/jquery.bxslider.js",
        "../node_modules/owlcarousel/owl-carousel/owl.carousel.js",
        "../node_modules/fancybox/dist/js/jquery.fancybox.js",
        "../node_modules/mediaelement/build/mediaelement-and-player.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
